generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String @default(cuid())
  email    String @unique
  password String
}

model Information {
  id        String   @id @default(cuid())
  title     String
  content   String
  image     String
  createdAt DateTime @default(now())
  updateAt  DateTime @updatedAt
}
